
-- Anti double load
if getgenv().MultiGameLoader then return end
getgenv().MultiGameLoader = true

local player = game:GetService("Players").LocalPlayer
local placeId = game.PlaceId

local gameScripts = {

    -- GAME 1
    [131623223084840] = function()
        print("Load Script: Escape Tsunami For Brainrots")
        loadstring(game:HttpGet(
            "https://raw.githubusercontent.com/Zyqrin/Escape-Tsunami-For-Brainrots/main/Main%20lua"
        ))()
    end,

    -- GAME 2
    [119987266683883] = function()
        print("Load Script: Survive LAVA for Brainrots")
        loadstring(game:HttpGet(
            "https://raw.githubusercontent.com/Zyqrin/Survive-LAVA-for-Brainrots/main/Main%20Lua"
        ))()
    end,

    -- GAME 3
    [138048509924404] = function()
        print("Load Script: Save Brainrots From LAVA")
        loadstring(game:HttpGet(
            "https://raw.githubusercontent.com/Zyqrin/Save-Brainrots-from-LAVA/main/Main%20Lua"
        ))()
    end,
}

local runScript = gameScripts[placeId]

if runScript then
    runScript()
else
    player:Kick("Game ini tidak didukung oleh loader.")
end
